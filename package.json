{
  "name": "healthappv2",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "main/main.js",
  "scripts": {
    "dev": "concurrently -n \"NEXT,EXPRESS\" -c \"yellow,green\" --kill-others \"npm run dev:next\" \"npm run dev:express\"",
    "dev:next": "next dev",
    "dev:express": "nodemon src/app/api/express/index.js",
    "dev:electron": "electron .",
    "build": "next build && electron-builder",
    "start": "next start",
    "lint": "next lint",
    "db": "docker exec -it postgres psql -U user -d healthApp"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@types/express": "^5.0.0",
    "@types/pg": "^8.11.10",
    "@types/pg-format": "^1.0.5",
    "@types/react-icons": "^2.2.7",
    "axios": "^1.7.7",
    "body-parser": "^1.20.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "electron-serve": "^2.1.1",
    "express": "^4.21.1",
    "framer-motion": "^11.11.9",
    "lucide-react": "^0.453.0",
    "next": "14.2.15",
    "nodemon": "^3.1.7",
    "open": "^10.1.0",
    "pg": "^8.13.0",
    "pg-format": "^1.0.4",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.3.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^9.0.1",
    "electron": "^33.0.2",
    "electron-builder": "^25.1.8",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
